alias acs 'apt-cache-search'
alias acp 'apt-cache-package'

alias apt-install 'sudo ~/bin/apt-fast.sh install'
alias apt-remove 'sudo apt-get remove'
alias apt-content 'dpkg -L'
alias apt-update 'sudo ~/bin/apt-fast.sh update'
alias apt-upgrade 'sudo ~/bin/apt-fast.sh upgrade'

alias apt-source 'apt-get source'
alias apt-build-dep 'sudo ~/bin/apt-fast.sh build-dep'
