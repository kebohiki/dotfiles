#!/bin/bash


cd ~
rm ~/.bashrc
cat -v .dotfiles/bash/bashrc/* >> ~/.bashrc

rm ~/.bash_it/aliases/custom.aliases.bash
cat -v ..dotfiles/bash/aliases/* >> ~/.bash_it/aliases/custom.aliases.bash
cat -v ..dotfiles/bash/aliases.windows/* >> ~/.bash_it/aliases/custom.aliases.bash

rm ~/.bash_it/custom/common.bash
cat -v ..dotfiles/bash/custom/* >> ~/.bash_it/custom/common.bash
cat -v ..dotfiles/bash/custom.windows/* >> ~/.bash_it/custom/common.bash
