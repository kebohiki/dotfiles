#!/bin/bash

curl -vsL get.freshshell.com > ~/.dotfiles/bootstrap/bootstrap-fresh
curl -vL https://github.com/bpinto/oh-my-fish/raw/master/tools/install.fish > ~/.dotfiles/bootstrap/bootstrap-oh-my-fish
curl -vsSL https://get.rvm.io > ~/.dotfiles/bootstrap/bootstrap-rvm

chmod 755 ~/.dotfiles/bootstrap/*
